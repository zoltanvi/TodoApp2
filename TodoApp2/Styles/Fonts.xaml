<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <FontFamily x:Key="SCE-PS3">pack://application;,,,/Fonts/#SCE-PS3 Rodin LATIN Regular</FontFamily>
    <FontFamily x:Key="Ubuntu">pack://application;,,,/Fonts/#Ubuntu Regular</FontFamily>
    <FontFamily x:Key="UbuntuLight">pack://application;,,,/Fonts/#Ubuntu Light</FontFamily>
    <FontFamily x:Key="UbuntuBold">pack://application;,,,/Fonts/#Ubuntu Bold</FontFamily>
    <FontFamily x:Key="SegoeMDL2Assets">pack://application;,,,/Fonts/#Segoe MDL2 Assets</FontFamily>

    <!--  Button icon characters  -->
    <system:String x:Key="SegoeIconMenu">&#xE700;</system:String>
    <system:String x:Key="SegoeIconSettings">&#xE713;</system:String>
    <system:String x:Key="SegoeIconMinimizeWindow">&#xE921;</system:String>
    <system:String x:Key="SegoeIconMaximizeWindow">&#xE922;</system:String>
    <system:String x:Key="SegoeIconCloseWindow">&#xE8BB;</system:String>
    <system:String x:Key="SegoeIconAdd">&#xE710;</system:String>
    <system:String x:Key="SegoeIconDateTime">&#xEC92;</system:String>
    <system:String x:Key="SegoeIconCalendar">&#xE787;</system:String>
    <system:String x:Key="SegoeIconPalette">&#xE790;</system:String>
    <system:String x:Key="SegoeIconTrash">&#xE74D;</system:String>
    <system:String x:Key="SegoeIconChangeTheme">&#xE793;</system:String>
    <system:String x:Key="SegoeIconAlwaysOnTop">&#xE727;</system:String>
    <system:String x:Key="SegoeIconBack">&#xE830;</system:String>
</ResourceDictionary>