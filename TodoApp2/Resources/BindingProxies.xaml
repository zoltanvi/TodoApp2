<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:local="clr-namespace:TodoApp2"
                    xmlns:core="clr-namespace:TodoApp2.Core;assembly=TodoApp2.Core"
                    xmlns:settings="clr-namespace:Modules.Settings.Views;assembly=Modules.Settings.Views"
                    xmlns:commonDD="clr-namespace:Modules.Common.Views.DragDrop;assembly=Modules.Common.Views"
                    xmlns:settingsContracts="clr-namespace:Modules.Settings.Contracts.ViewModels;assembly=Modules.Settings.Contracts"
                    xmlns:view="clr-namespace:TodoApp2.WindowHandling"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <settings:AppSettingsBindingProxy x:Key="AppSettings" AppSettings="{x:Static settingsContracts:AppSettings.Instance}"/>
    <view:UIScalerBindingProxy x:Key="UIScaler" UIScaler="{x:Static core:UIScaler.Instance}" />
    <commonDD:CustomDropHandlerBindingProxy x:Key="CustomDropHandler" Handler="{x:Static commonDD:CustomDropHandler.Instance}" />

</ResourceDictionary>