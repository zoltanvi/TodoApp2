<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="TransparentBrush" Color="{DynamicResource Transparent}" />

    <LinearGradientBrush x:Key="TransparentPatternBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0" Color="#888888" />
        <GradientStop Offset="0.5" Color="#888888" />
        <GradientStop Offset="0.5" Color="#FFFFFFFF" />
        <GradientStop Offset="1" Color="#FFFFFFFF" />
    </LinearGradientBrush>

</ResourceDictionary>
